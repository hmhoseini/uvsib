		"mace = uvsib.codes.mace.calculation:MACECalculation",
		"upet = uvsib.codes.upet.calculation:uPETCalculation",
		"mh = uvsib.codes.minimahopping.calculation:MinimaHoppingCalculation"
	],
	"aiida.parsers": [
		"mattergen_parser = uvsib.codes.mattergen.parser:MatterGenParser",
		"mattersim_parser = uvsib.codes.mattersim.parser:MatterSimParser",
		"mace_parser = uvsib.codes.mace.parser:MACEParser",
		"upet_parser = uvsib.codes.upet.parser:uPETParser",
		"minimahopping_parser = uvsib.codes.minimahopping.parser:MinimaHoppingParser"
	],
	"aiida.workflows": [
		"mattergen.base = uvsib.codes.mattergen.workchain:MatterGenBaseWorkChain",
		"mattergen.csp = uvsib.codes.mattergen.workchain:MatterGenCSPWorkChain",
		"mattersim = uvsib.codes.mattersim.workchain:MatterSimWorkChain",
		"mace = uvsib.codes.mace.workchain:MACEWorkChain",
		"upet = uvsib.codes.upet.workchain:uPETWorkChain",
		"minimahopping = uvsib.codes.minimahopping.workchain:MinimaHoppingWorkChain",
		"phasediagram = uvsib.workchains.phase_diagram:PhaseDiagramMLWorkChain",
		"pdverification = uvsib.workchains.pd_verification:PDVerificationWorkChain",
		"bandalignment = uvsib.workchains.band_alignment:BandAlignmentWorkChain",
		"csp = uvsib.workchains.csp:CSPWorkChain",
		"gen = uvsib.workchains.gen:GeneratorWorkChain",
		"surfacebuilder = uvsib.workchains.surface_builder:SurfaceBuilderWorkChain",
		"adsorbates = uvsib.workchains.adsorbates:AdsorbatesWorkChain"
	]
    }
}
